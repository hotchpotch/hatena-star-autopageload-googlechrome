{
  "name": "HatenaStar AutoPageLoad",
  "id": "cd35a0eb91f748188612c249dc62cc77",
  "description": "AutoPagerize/AutoPatchWork で継ぎ足したページにもはてなスターをロード",
  "version": "0.0.2",
  "content_scripts": [
    {
      "run_at": "document_end",
      "all_frames": false,
      "js": [
        "HatenaStarLoad.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
